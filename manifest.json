{
	"manifest_version": 2,
	"name": "DC-Extension",
	"description": "An extension for Chrome that gives an overview of some of the main sites content",
	"author": "Alex Knopp",
	"version": "1.0",
	"permissions": ["storage"],
	"content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "css": ["background.css"],
      "js": ["jquery.min.js", "background.js"]
    }],
	"browser_action": {
		"default_title": "DC Departments Overview",
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png"
		}
	}	

}